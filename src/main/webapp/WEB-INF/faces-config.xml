	</navigation-rule>	
	
	<!-- 
	********************************************************************
	ITEMS LIST
	********************************************************************
	-->
	<navigation-rule>
		<from-view-id>/public/item/itemsList.jspx</from-view-id>
		<navigation-case>
			<from-outcome>nextPage</from-outcome>
			<to-view-id>/public/item/itemsList.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>previousPage</from-outcome>
			<to-view-id>/public/item/itemsList.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>/public/item/itemsList.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clearSearch</from-outcome>
			<to-view-id>/public/item/itemsList.jspx</to-view-id>
		</navigation-case>	
	</navigation-rule>
	
