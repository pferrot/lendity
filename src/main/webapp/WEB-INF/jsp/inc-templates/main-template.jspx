<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:o="http://myfaces.apache.org/orchestra"
      xmlns:c="http://java.sun.com/jstl/core">

<head>
    <!-- page title param passed in by param -->
    <title><h:outputText value="#{messages.general_mainTitle}"/></title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta http-equiv="Expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
	<meta http-equiv="Pragma" content="no-cache" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/reset-fonts.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/gt-styles.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/gt-fixed.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/override.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/custom.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/jquery/jquery-ui-1.7.3.custom.css" type="text/css" media="screen, projection" />
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/jquery/jquery-1.3.2.js"></script>	
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/jquery/jquery-ui-1.7.custom.min.js"></script>
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/jquery/qtip/jquery.qtip-1.0.0-rc3.js"></script>
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/utils.js"></script>
	
	
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/jquery/superfish/superfish.js"></script>
	<script type="text/javascript" src="#{facesContext.externalContext.requestContextPath}/public/javascript/jquery/superfish/hoverIntent.js"></script>
	<link rel="stylesheet" href="#{facesContext.externalContext.requestContextPath}/public/css/jquery/superfish/superfish.css" type="text/css" media="screen, projection" />
	
	
	<script type="text/javascript">
	  var $j = jQuery.noConflict();
	</script>	

	<script type="text/javascript">
		colorErrorFields();
	</script>
	
	<script>
	// initialise plugins
	$j(document).ready(function() {
		$j('ul.sf-menu').superfish();
	});
	</script>   

<!-- 	
  <script type="text/javascript">
    $j(document).ready(function() {
        $j(":text:first").focus();
    });  
  </script>
 -->	
	
</head>
<body class="gt-fixed">


		<!-- head -->
		<div class="gt-hd clearfix">
			
			<!-- head top -->
			<div class="gt-hd-top clearfix">
			
				<div class="gt-fixed-wrap clearfix">
			
				<!-- logo -->
				<div class="gt-logo">
					<h:outputText value="#{messages.general_mainTitle}"/>
				</div>
				<!-- / logo -->
				
				
				<!-- utility box -->
				<div class="gt-util-box">
					<div class="gt-util-box-inner">
						<c:if test="#{authenticationController.loggedIn}" var="loggedInUser"/>
						<c:if test="${loggedInUser}">
							<p><strong><h:outputText value="#{authenticationController.currentUserDisplayName}"/></strong></p>
							<p>
                        		<h:outputLink value="#{authenticationController.logoutURL}">
	            					<h:outputText value="#{messages.logout_logout}"/>
				         		</h:outputLink>
		         			</p>
		         			<p>
                        		<h:outputLink value="#{authenticationController.myProfileURL}">
	            					<h:outputText value="#{messages.myProfile_myProfile}"/>
				         		</h:outputLink>
		         			</p>
	         			</c:if>
	         			<c:if test="${not (loggedInUser)}">
		         			<p>
                        		<h:outputLink value="#{authenticationController.loginURL}">
                        			<h:outputText value="#{messages.login_login2}"/>
                        		</h:outputLink>
	                        </p>
	         			</c:if>
					</div>
				</div>
				<!-- / utility box -->
				
				</div>
			
			</div><!-- /head top -->
			
			<!-- navbar -->
			<div class="gt-navbar clearfix">
			
				<div class="gt-fixed-wrap clearfix">
				
				<!-- navigation -->
				<div>







<ul class="sf-menu">
	<li>
	    <h:outputLink value="#{authenticationController.homeURL}">
	        <h:outputText value="#{messages.menu_home}" />
	    </h:outputLink>
	</li>
	<c:if test="${loggedInUser}">
    <li>
        <h:outputLink value="#{authenticationController.myItemsURL}">
        	<h:outputText value="#{messages.menu_items}"/>
        </h:outputLink>
		<ul class="sf-menu-sub1">
			<li>
			    <h:outputLink value="#{authenticationController.myItemsURL}">
				    <h:outputText value="#{messages.menu_myItems}" />
			    </h:outputLink>
			</li>
			<li>
			    <h:outputLink value="#{authenticationController.myBorrowedItemsURL}">
		                        	<h:outputText value="#{messages.menu_myBorrowedItems}"/>
		                        </h:outputLink>
			</li>
			<li>
			   <h:outputLink value="#{authenticationController.myConnectionsItemsURL}">
		                        	<h:outputText value="#{messages.menu_myConnectionsItems}"/>
		                        </h:outputLink>
			</li>
			<li>
			    <h:outputLink value="#{authenticationController.myItemsURL}">
				    <h:outputText value="#{messages.menu_myItems}" />
			    </h:outputLink>
			</li>
	    </ul>
	</li>
	<li>
      	<h:outputLink value="#{authenticationController.myConnectionsURL}">
            <h:outputText value="#{messages.menu_friends}"/>
        </h:outputLink>
		<ul class="sf-menu-sub1">
			<li>
			   <h:outputLink value="#{authenticationController.myConnectionsURL}">
               	  <h:outputText value="#{messages.menu_myConnections}"/>
               </h:outputLink>
			</li>
			<li>
			    <h:outputLink value="#{authenticationController.personsURL}">
		            <h:outputText value="#{messages.menu_persons}"/>
		        </h:outputLink>
		    </li>
			<li>
			    <h:outputLink value="#{authenticationController.myPendingConnectionRequestsURL}">
		                        	<h:outputText value="#{messages.menu_myPendingConnectionRequests}"/>
		                        </h:outputLink>
			</li>
			<li>
			    <h:outputLink value="#{authenticationController.myBannedPersonsURL}">
		                        	<h:outputText value="#{messages.menu_myBannedPersons}"/>
		                        </h:outputLink>
			</li>
			<li>
			    <h:outputLink value="#{authenticationController.myPendingConnectionRequests2URL}">
		                        	<h:outputText value="#{messages.menu_myPendingConnectionRequests2}"/>
		                        </h:outputLink>
			</li>
	    </ul>
	</li>
	</c:if>
</ul>

















				</div><!-- / navigation -->
				
				</div>
				
			</div><!-- /navbar -->			
		</div>
		<!-- / head -->
  		
  		<ui:insert name="content">Default Content</ui:insert>
		<!-- footer -->
		<div class="gt-footer"><div class="gt-footer-inner">
			<p>&#169; 2010 pf</p>
		</div></div>
		<!-- /footer -->
</body>
</html>
